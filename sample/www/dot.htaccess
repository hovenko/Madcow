SetEnv ENVIRONMENT 'development'

RewriteEngine On
RewriteBase /~user/madcow-sample/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /~user/madcow-sample/index.php

